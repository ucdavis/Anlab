@using Anlab.Core.Models
@model Order
@{
    var order = Model.GetOrderDetails();
    var protocol = Context.Request.Scheme;
    var host = Context.Request.Host;
    var path = $"{protocol}://{host}";
    var image = $"{path}/{Url.Content("~/Images/anlabEmailLogo.jpg")}";
    var hiddenCosts = 0.0M;
    var creator = Model.Creator;
    var ActionText = "";
    var h2Text = $"Order Number {Model.Id}";
    var h4Text = "";
    if (Model.IsDeleted)
    {
        ActionText = "set to DELETED";
    }
    else
    {
        if (Model.Status == OrderStatusCodes.Created)
        {
            ActionText = "set back to Created status";

        }
        else
        {
            ActionText = "??? Contact Tech support (Jason Sylvestre)";
        }
    }
    if (!string.IsNullOrWhiteSpace(Model.RequestNum))
    {
        h4Text = $"Work Request {Model.RequestNum} (But this might get changed)";
    }
}

