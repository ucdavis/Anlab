@using Anlab.Core.Models
@using AnlabMvc.Extensions
@model Order
@{
    var order = Model.GetOrderDetails();
    var protocol = Context.Request.Scheme;
    var host = Context.Request.Host;
    var path = $"{protocol}://{host}";
    var image = $"{path}/{Url.Content("~/Images/anlab170.png")}";
    var createdBy = Model.Creator.Name;
    var hiddenCosts = 0.0M;
    var paymentDetail = string.Empty;
    if (Model.PaymentType == PaymentTypeCodes.Other || Model.PaymentType == PaymentTypeCodes.UcOtherAccount)
    {
        if (order.OtherPaymentInfo != null)
        {
            paymentDetail = $" ({order.OtherPaymentInfo.PaymentType})";
        }
        else
        {
            paymentDetail = $" (Unknown Payment Type)";
        }

    }
}

